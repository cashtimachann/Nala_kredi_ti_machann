# Branch Manager Production Environment
# Konfigirasyon pou manager branch yo sou branch.nalakreditimachann.com

# API URL - Use relative path because nginx proxy handles routing
REACT_APP_API_URL=/api

# Branch Domain Configuration
REACT_APP_DOMAIN=branch.nalakreditimachann.com
REACT_APP_APP_TYPE=branch

# Disable source maps in production for security
GENERATE_SOURCEMAP=false

# Sentry monitoring (optional)
REACT_APP_SENTRY_DSN=
REACT_APP_SENTRY_TRACES_SAMPLE_RATE=0
